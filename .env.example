# COGNIFLOW Environment Configuration
# Copy this file to .env.local for local development

# ============================================================================
# AI PROVIDER API KEYS (Local Development Only)
# ============================================================================
# WARNING: These keys are exposed to the browser in development.
# For production, implement a backend API proxy.

# Google Gemini
VITE_GEMINI_API_KEY=

# OpenAI
VITE_OPENAI_API_KEY=

# Anthropic Claude
VITE_ANTHROPIC_API_KEY=

# HuggingFace
VITE_HUGGINGFACE_API_KEY=

# OpenRouter
VITE_OPENROUTER_API_KEY=

# Groq
VITE_GROQ_API_KEY=

# ============================================================================
# PRODUCTION API PROXY (Recommended)
# ============================================================================
# Instead of exposing keys, route requests through your backend
# VITE_API_PROXY_URL=https://your-api.example.com/ai/proxy

# ============================================================================
# OPTIONAL: ANALYTICS & MONITORING
# ============================================================================
# VITE_SENTRY_DSN=
# VITE_ANALYTICS_ID=

# ============================================================================
# DEVELOPMENT SETTINGS
# ============================================================================
VITE_DEV_MODE=true
VITE_LOG_LEVEL=debug
